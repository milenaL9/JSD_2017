Rodoslov:
    "rodoslov"
    naslov = STRING
    rodonacelnik = Rodonacelnik
;

Rodonacelnik:
    "Rodonacelnik" "{"
    ime = Ime
    slika = STRING
    titule += Titula
    potomci *= Vladar
    "Rodonacelnik" "}"
;

Vladar:
    "Vladar" "{"
    ime = Ime
    slika = STRING
    titule += Titula
    potomci *= Vladar
    "Vladar" "}"
;

Ime:
    id = ID naziv = STRING labela = STRING vrednost = STRING
;

Titula:
    "titula" "{"
    id = ID
    naziv = STRING
    labela = STRING
    tip = TitulaTip
    teritorija=Teritorija
    "titula" "}"
;

TitulaTip:
    'veliki zupan'|'kralj'|'car'|'carica'|'kneginja'|'arhiepiskop'|'nema'
;

Teritorija:
    "teritorija" "{"
    id = ID
    labela = STRING
    nazivTeritorije = STRING
    pocetakVladavine = PocetakVladavine
    krajVladavine = KrajVladavine
    "teritorija" "}"
;

PocetakVladavine:
    "pocetakVladavine" id = ID naziv = STRING labela = STRING vrednost = INT
;

KrajVladavine:
    "krajVladavine" id = ID naziv = STRING labela = STRING vrednost = INT
;